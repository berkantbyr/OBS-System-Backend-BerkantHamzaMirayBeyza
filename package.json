{
  "name": "university-obs-backend",
  "version": "1.0.0",
  "description": "Üniversite Öğrenci Bilgi Sistemi - Backend API",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node scripts/kill-port.js && nodemon",
    "dev:safe": "node scripts/kill-port.js && nodemon",
    "kill-port": "node scripts/kill-port.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "node src/seeders/seedPart2Data.js",
    "seed:part2": "node src/seeders/seedPart2Data.js",
    "seed:production": "node scripts/seed-production.js",
    "seed:cli": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "create-admin": "node scripts/create-admin.js",
    "create-superadmin": "node scripts/create-superadmin.js"
  },
  "keywords": [
    "university",
    "student",
    "management",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^10.32.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.15.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.17.2",
    "redis": "^5.10.0",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
